<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/white"
    tools:context=".HomeFragment">
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_horizontal_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.05" />

    <ImageView
        android:id="@+id/img_7"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/profile"
        android:layout_marginTop="10dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/guideline_horizontal_2">
    </ImageView>

    <TextView
        android:id="@+id/txt_9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="Belda Putri Pramono"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/img_7">
    </TextView>

    <TextView
        android:id="@+id/txt_10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="beldapramono823@gmail.com"
        android:textColor="@color/grey"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/txt_9"
        tools:ignore="TextSizeCheck"></TextView>

    <TextView
        android:id="@+id/txt_11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="55dp"
        android:text="Edit Profile"
        android:textColor="@color/black"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/txt_10"
        tools:ignore="TextSizeCheck"></TextView>

    <TextView
        android:id="@+id/txt_12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:text="Add Pin"
        android:textColor="@color/black"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@id/txt_11"
        app:layout_constraintTop_toBottomOf="@id/txt_11"
        tools:ignore="TextSizeCheck"></TextView>

    <TextView
        android:id="@+id/txt_13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="Settings"
        android:textColor="@color/black"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@id/txt_12"
        app:layout_constraintTop_toBottomOf="@id/txt_12"
        tools:ignore="TextSizeCheck"></TextView>

    <TextView
        android:id="@+id/txt_14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="Logout"
        android:textColor="@color/red"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@id/txt_12"
        app:layout_constraintTop_toBottomOf="@id/txt_13"
        tools:ignore="TextSizeCheck"></TextView>

    <ImageView
        android:id="@+id/icon_4"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginEnd="20dp"
        android:layout_marginTop="52dp"
        android:src="@drawable/baseline_account_circle_24"
        app:layout_constraintTop_toBottomOf="@id/txt_10"
        app:layout_constraintStart_toStartOf="@id/txt_10">
    </ImageView>

    <ImageView
        android:id="@+id/icon_5"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="12dp"
        android:src="@drawable/baseline_password_24"
        app:layout_constraintTop_toBottomOf="@id/icon_4"
        app:layout_constraintStart_toStartOf="@id/icon_4">
    </ImageView>

    <ImageView
        android:id="@+id/icon_6"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="12dp"
        android:src="@drawable/baseline_settings_24"
        app:layout_constraintTop_toBottomOf="@id/txt_12"
        app:layout_constraintStart_toStartOf="@id/icon_5">
    </ImageView>

    <ImageView
        android:id="@+id/icon_7"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="12dp"
        android:src="@drawable/baseline_logout_24"
        app:layout_constraintTop_toBottomOf="@id/txt_13"
        app:layout_constraintStart_toStartOf="@id/icon_6">
    </ImageView>

    <ImageView
        android:id="@+id/icon_8"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="52dp"
        android:src="@drawable/arrow_forwards"
        app:layout_constraintTop_toBottomOf="@id/txt_10"
        app:layout_constraintStart_toEndOf="@id/txt_11">
    </ImageView>

    <ImageView
        android:id="@+id/icon_9"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginTop="100dp"
        android:src="@drawable/arrow_forwards"
        app:layout_constraintTop_toBottomOf="@id/txt_10"
        app:layout_constraintStart_toStartOf="@id/icon_8">
    </ImageView>

    <ImageView
        android:id="@+id/icon_10"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginTop="25dp"
        android:src="@drawable/arrow_forwards"
        app:layout_constraintTop_toBottomOf="@id/icon_9"
        app:layout_constraintStart_toStartOf="@id/icon_9">
    </ImageView>

    <ImageView
        android:id="@+id/icon_11"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginTop="25dp"
        android:src="@drawable/arrow_forwards"
        app:layout_constraintStart_toStartOf="@id/icon_10"
        app:layout_constraintTop_toBottomOf="@id/txt_13">
    </ImageView>




</androidx.constraintlayout.widget.ConstraintLayout>